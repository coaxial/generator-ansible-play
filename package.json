{
  "name": "generator-ansible-play",
  "version": "0.1.2",
  "description": " Ansible playbook and associated `make` commands generator for Yeoman",
  "homepage": "github.com/coaxial/generator-ansible-play",
  "author": {
    "name": "coaxial",
    "email": "hi@64b.it",
    "url": "https://64b.it"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "ansible",
    "play",
    "playbook",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.24.1",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "nsp": "^3.2.1",
    "prettier": "^1.14.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.9.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "change-case": "^3.0.2",
    "generator-license": "^5.4.0",
    "git-remote-origin-url": "^2.0.0",
    "mkdirp": "^0.5.1",
    "ramda": "^0.25.0",
    "yeoman-generator": "^3.1.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90,
          "trailingComma": "all"
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "coaxial/generator-ansible-play",
  "license": "MIT"
}
